{
	"name": "exolnet/php-composer-template",
	"description": "PHP Composer Template",
	"type": "library",
	"keywords": [
		""
	],
	"homepage": "https://github.com/exolnet/php-composer-template",
	"license": "MIT",
	"authors": [
		{
			"name": "Your name",
			"email": "youraddress@exolnet.com"
		}
	],
	"support": {
		"issues": "https://github.com/exolnet/php-composer-template/issues"
	},
	"require": {
		"php": ">=5.4.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4"
	},
	"autoload": {
		"psr-4": {
			"MyNamespace\\": "src/MyNamespace"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"MyNamespace\\Test\\": "tests/MyNamespace"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1-dev"
		}
	}
}
